<!DOCTYPE html>
<html>
<head>
  <title>Beyt el Mouneh Online Store</title>
<!-- START:stylesheet -->
  <%= stylesheet_link_tag "store", :media => "all" %><!-- <label id="code.slt"/> -->
<!-- END:stylesheet -->
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %><!-- <label id="code.csrf"/> -->
</head>

<body id="store">
  <div id="logo">
    <%= image_tag("beyt_el_mouneh.jpg", :size => '90x60') %>
  </div>
  <div id="banner">
    <%= @page_title || "Beyt el Mouneh Online Store" %><!-- <label id="code.depot.e.title"/> -->
  </div>
  <div id="columns">
    <div id="side">
      <% if @cart %>
        <%= hidden_div_if(@cart.line_items.empty?, :id => "cart") do %>
          <%= render @cart %>
        <% end %>
      <% end %>
      <a href="/">Home</a><br />
      <a href="/about">About</a><br />
      <a href="/contact">Contact</a><br />
      <br />
      <% if signed_in? %>
        <a href="/signout">Log out</a><br />
      <% else %>
        <a href="/signin">Log in</a><br />
        <a href="/signup">Register</a><br />
      <% end %>
    </div>
    <div id="main">
      <%= yield %><!-- <label id="code.depot.e.include"/> -->
    </div>
  </div>

</body>
</html>

